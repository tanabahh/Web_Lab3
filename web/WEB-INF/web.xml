<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>

    </servlet>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>*.xhtml</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.xhtml</welcome-file>
    </welcome-file-list>

    <context-param>
        <param-name>jdbc-protocol-name</param-name>
        <param-value>postgresql</param-value>
    </context-param>
    <context-param>
        <param-name>jdbc-host</param-name>
        <param-value>localhost</param-value>
    </context-param>
    <context-param>
        <param-name>jdbc-port</param-name>
        <param-value>5432</param-value>
    </context-param>
    <context-param>
        <param-name>jdbc-database</param-name>
        <param-value></param-value>
    </context-param>
    <context-param>
        <param-name>jdbc-user</param-name>
        <param-value></param-value>
    </context-param>
    <context-param>
        <param-name>jdbc-password</param-name>
        <param-value></param-value>
    </context-param>

</web-app>